#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/Library/CloudStorage/OneDrive-BYU-Idaho/Documents/MICROSOFT OFFICE-PDF/MY BYU/BYU DEGREE/CSE340_Web Backend Development/CSE340/node_modules/.pnpm/nodemon@3.1.0/node_modules/nodemon/bin/node_modules:/Users/apple/Library/CloudStorage/OneDrive-BYU-Idaho/Documents/MICROSOFT OFFICE-PDF/MY BYU/BYU DEGREE/CSE340_Web Backend Development/CSE340/node_modules/.pnpm/nodemon@3.1.0/node_modules/nodemon/node_modules:/Users/apple/Library/CloudStorage/OneDrive-BYU-Idaho/Documents/MICROSOFT OFFICE-PDF/MY BYU/BYU DEGREE/CSE340_Web Backend Development/CSE340/node_modules/.pnpm/nodemon@3.1.0/node_modules:/Users/apple/Library/CloudStorage/OneDrive-BYU-Idaho/Documents/MICROSOFT OFFICE-PDF/MY BYU/BYU DEGREE/CSE340_Web Backend Development/CSE340/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/Library/CloudStorage/OneDrive-BYU-Idaho/Documents/MICROSOFT OFFICE-PDF/MY BYU/BYU DEGREE/CSE340_Web Backend Development/CSE340/node_modules/.pnpm/nodemon@3.1.0/node_modules/nodemon/bin/node_modules:/Users/apple/Library/CloudStorage/OneDrive-BYU-Idaho/Documents/MICROSOFT OFFICE-PDF/MY BYU/BYU DEGREE/CSE340_Web Backend Development/CSE340/node_modules/.pnpm/nodemon@3.1.0/node_modules/nodemon/node_modules:/Users/apple/Library/CloudStorage/OneDrive-BYU-Idaho/Documents/MICROSOFT OFFICE-PDF/MY BYU/BYU DEGREE/CSE340_Web Backend Development/CSE340/node_modules/.pnpm/nodemon@3.1.0/node_modules:/Users/apple/Library/CloudStorage/OneDrive-BYU-Idaho/Documents/MICROSOFT OFFICE-PDF/MY BYU/BYU DEGREE/CSE340_Web Backend Development/CSE340/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
